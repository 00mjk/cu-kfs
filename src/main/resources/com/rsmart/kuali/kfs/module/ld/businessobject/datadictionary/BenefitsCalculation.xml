  <bean id="BenefitsCalculation" parent="BenefitsCalculation-parentBean">
    <property name="businessObjectClass" value="com.rsmart.kuali.kfs.module.ld.businessobject.BenefitsCalculation"/>     
    <property name="baseBusinessObjectClass" value="org.kuali.kfs.module.ld.businessobject.BenefitsCalculation"/>

    <property name="attributes">
      <list merge="true">
        <ref bean="BenefitsCalculation-accountCodeOffset"/>      
        <ref bean="BenefitsCalculation-objectCodeOffset"/>
      </list>
    </property>
    
    <property name="relationships">
	  <list merge="true">
	    <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="laborAccountOffset"/>
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="extension.accountCodeOffset" p:targetName="accountNumber"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="chartOfAccountsCode" p:targetName="chartOfAccountsCode"/>
            </list>
          </property>
        </bean>
	    <bean parent="RelationshipDefinition">
          <property name="objectAttributeName" value="laborObjectCodeOffset"/>
          <property name="primitiveAttributes">
            <list>
              <bean parent="PrimitiveAttributeDefinition" p:sourceName="extension.objectCodeOffset" p:targetName="financialObjectCode"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="chartOfAccountsCode" p:targetName="chartOfAccountsCode"/>
              <bean parent="SupportAttributeDefinition" p:sourceName="universityFiscalYear" p:targetName="universityFiscalYear"/>
            </list>
          </property>
        </bean>
	  </list>
	</property>
	
  </bean>

<!-- Attribute Definitions -->

  <bean id="BenefitsCalculation-accountCodeOffset" parent="BenefitsCalculationExtension-accountCodeOffset-parentBean">
    <property name="name" value="extension.accountCodeOffset"/>
  </bean>

  <bean id="BenefitsCalculation-objectCodeOffset" parent="BenefitsCalculationExtension-objectCodeOffset-parentBean">
    <property name="name" value="extension.objectCodeOffset"/>
  </bean>
  
  <bean id="BenefitsCalculation-positionFringeBenefitObjectCode" parent="BenefitsCalculation-positionFringeBenefitObjectCode-parentBean">
    <property name="label" value="Labor Benefit Object Code"/>
  </bean>

<!-- Business Object Inquiry Definition -->


  <bean id="BenefitsCalculation-inquiryDefinition" parent="BenefitsCalculation-inquiryDefinition-parentBean">
    <property name="presentationControllerClass" value="org.kuali.kfs.module.ld.document.authorization.BenefitsCalculationMaintenanceDocumentPresentationController" />
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value=""/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="universityFiscalYear"/>
              <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
              <bean parent="FieldDefinition" p:attributeName="positionBenefitTypeCode"/>
              <bean parent="FieldDefinition" p:attributeName="laborBenefitRateCategoryCode"/>
              <bean parent="FieldDefinition" p:attributeName="positionFringeBenefitObjectCode"/>
              <bean parent="FieldDefinition" p:attributeName="positionFringeBenefitPercent"/>
              <bean parent="FieldDefinition" p:attributeName="extension.accountCodeOffset"/>
              <bean parent="FieldDefinition" p:attributeName="extension.objectCodeOffset"/>
              <bean parent="FieldDefinition" p:attributeName="active"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

<!-- Business Object Lookup Definition -->

  <bean id="BenefitsCalculation-lookupDefinition" parent="BenefitsCalculation-lookupDefinition-parentBean">
    <property name="lookupableID" value="ldBenefitsCalculationLookupable"/>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="universityFiscalYear" p:defaultValueFinderClass="com.rsmart.kuali.kfs.module.ld.businessobject.defaultvalue.FiscalYearFinder"/>
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="positionBenefitTypeCode"/>
        <bean parent="FieldDefinition" p:attributeName="laborBenefitRateCategoryCode"/>
        <bean parent="FieldDefinition" p:attributeName="positionFringeBenefitObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="positionFringeBenefitPercent"/>
        <bean parent="FieldDefinition" p:attributeName="extension.accountCodeOffset"/>
        <bean parent="FieldDefinition" p:attributeName="extension.objectCodeOffset"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="laborBenefitsCalculationViewer"/>
        <bean parent="FieldDefinition" p:attributeName="universityFiscalYear"/>
        <bean parent="FieldDefinition" p:attributeName="chartOfAccountsCode"/>
        <bean parent="FieldDefinition" p:attributeName="positionBenefitTypeCode"/>
        <bean parent="FieldDefinition" p:attributeName="laborBenefitRateCategoryCode"/>
        <bean parent="FieldDefinition" p:attributeName="positionFringeBenefitObjectCode"/>
        <bean parent="FieldDefinition" p:attributeName="extension.accountCodeOffset"/>
        <bean parent="FieldDefinition" p:attributeName="extension.objectCodeOffset"/>
        <bean parent="FieldDefinition" p:attributeName="positionFringeBenefitPercent"/>
        <bean parent="FieldDefinition" p:attributeName="active"/>
      </list>
    </property>
  </bean>

</beans>
