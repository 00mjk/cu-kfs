<?xml version="1.0" encoding="UTF-8"?>
<!--
    Default output definition for 1042S tax processing.
    This definition is for the tax year 2014 format.
 -->
<taxOutputDefinition xmlns="http://www.kuali.org/kfs/tax/taxOutputDefinition" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <!--
        ======================================================================
        This section represents a 1042S header record (fields A1 through A13).
        ======================================================================
     -->
    <section length="256" hasExactLength="true">
        <field name="A1" length="40" type="DERIVED" value="vendorLastName" /><!-- A1 :: Last Name -->
        <field name="A2" length="40" type="DERIVED" value="vendorFirstName" /><!-- A2 :: First Name -->
        <field name="A3" length="40" type="BLANK" /><!-- A3 :: Middle Initials -->
        <field name="A4" length="10" type="BLANK" /><!-- A4 :: System Generated ID Number -->
        <field name="A5" length="11" type="DERIVED" value="ssn" /><!-- A5 :: Social Security Number [999-99-9999] -->
        <field name="A6" length="15" type="BLANK" /><!-- A6 :: School Assigned ID Number -->
        <field name="A7" length="10" type="BLANK" /><!-- A7 :: Date of Birth -->
        <field name="A8" length="11" type="DERIVED" value="itin" /><!-- A8 :: ITIN [999-99-9999] -->
        <field name="A9" length="15" type="BLANK" /><!-- A9 :: Payroll ID -->
        <field name="A10" length="15" type="DETAIL" value="payeeId" /><!-- A10 :: Financial/AP ID -->
        <field name="A11" length="15" type="BLANK" /><!-- A11 :: Student ID -->
        <field name="A12" length="15" type="BLANK" /><!-- A12 :: VISA System ID -->
        <field name="A13" length="19" type="VENDOR" value="vendorGIIN" /><!-- A13 :: Global Intermediary Identification Number (GIIN) [XXXXXX.XXXXX.XX.XXX] -->
    </section>
    <!--
        ====================================================================================================================
        This section represents a 1042S biographic record (header record fields A1 through A13, plus fields B1 through B99).
        ====================================================================================================================
     -->
    <section length="1344" hasExactLength="true">
        <field name="B1" length="2" type="DERIVED" value="chapter3StatusCode" /><!-- B1 :: Chapter 3 Status Code -->
        <field name="B2" length="2" type="VENDOR" value="vendorChapter4StatusCode" /><!-- B2 :: Chapter 4 Status Code -->
        <field name="B3" length="10" type="BLANK" /><!-- B3 :: Title -->
        <field name="B4" length="10" type="BLANK" /><!-- B4 :: Title Suffix -->
        <field name="B5" length="40" type="BLANK" /><!-- B5 :: Maiden Name -->
        <field name="B6" length="1" type="BLANK" /><!-- B6 :: Active Individual -->
        <field name="B7" length="1" type="BLANK" /><!-- B7 :: Individual Married -->
        <field name="B8" length="1" type="BLANK" /><!-- B8 :: Spouse Here -->
        <field name="B9" length="1" type="BLANK" /><!-- B9 :: Spouse Working -->
        <field name="B10" length="1" type="BLANK" /><!-- B10 :: Spouse_dependant -->
        <field name="B11" length="2" type="BLANK" /><!-- B11 :: Dependents -->
        <field name="B12" length="2" type="BLANK" /><!-- B12 :: Dependents Here -->
        <field name="B13" length="2" type="BLANK" /><!-- B13 :: Dependents Citizens -->
        <field name="B14" length="1" type="BLANK" /><!-- B14 :: Applied for SSN/ITIN -->
        <field name="B15" length="100" type="DERIVED" value="vendorEmailAddress" /><!-- B15 :: Email Address -->
        <field name="B16" length="25" type="BLANK" /><!-- B16 :: Department -->
        <field name="B17" length="40" type="BLANK" /><!-- B17 :: Occupation -->
        <field name="B18" length="40" type="DERIVED" value="vendorUSAddressLine1" /><!-- B18 :: US Address (Line 1) -->
        <field name="B19" length="40" type="VENDOR_US_ADDRESS" value="vendorLine2Address" /><!-- B19 :: US Address (Line 2) -->
        <field name="B20" length="40" type="BLANK" /><!-- B20 :: US Address (Line 3) -->
        <field name="B21" length="40" type="VENDOR_US_ADDRESS" value="vendorCityName" /><!-- B21 :: US City -->
        <field name="B22" length="2" type="VENDOR_US_ADDRESS" value="vendorStateCode" /><!-- B22 :: US State -->
        <field name="B23" length="10" type="VENDOR_US_ADDRESS" value="vendorZipCode" /><!-- B23 :: US Zip Code -->
        <field name="B24" length="15" type="BLANK" /><!-- B24 :: US Home Telephone Number -->
        <field name="B25" length="15" type="BLANK" /><!-- B25 :: US Work Telephonse Number -->
        <field name="B26" length="10" type="BLANK" /><!-- B26 :: US FAX Number -->
        <field name="B27" length="1" type="BLANK" /><!-- B27 :: Use US Address -->
        <field name="B28" length="40" type="DERIVED" value="vendorForeignAddressLine1" /><!-- B28 :: Foreign Address (Line 1) -->
        <field name="B29" length="40" type="VENDOR_FOREIGN_ADDRESS" value="vendorLine2Address" /><!-- B29 :: Foreign Address (Line 2) -->
        <field name="B30" length="40" type="BLANK" /><!-- B30 :: Foreign Address (Line 3) -->
        <field name="B31" length="10" type="VENDOR_FOREIGN_ADDRESS" value="vendorZipCode" /><!-- B31 :: Foreign City Postal Code -->
        <field name="B32" length="40" type="VENDOR_FOREIGN_ADDRESS" value="vendorCityName" /><!-- B32 :: Foreign City -->
        <field name="B33" length="10" type="BLANK" /><!-- B33 :: Foreign Region Postal Code -->
        <field name="B34" length="30" type="BLANK" /><!-- B34 :: Foreign Region -->
        <field name="B35" length="2" type="VENDOR_FOREIGN_ADDRESS" value="vendorCountryCode" /><!-- B35 :: Foreign Country Code -->
        <field name="B36" length="25" type="BLANK" /><!-- B36 :: Foreign Country Name (NOTE: Should be non-blank if B35 is blank or invalid) -->
        <field name="B37" length="2" type="BLANK" /><!-- B37 :: Citizenship Country Code -->
        <field name="B38" length="25" type="BLANK" /><!-- B38 :: Citizenship Country Name -->
        <field name="B39" length="1" type="BLANK" /><!-- B39 :: US Citizen -->
        <field name="B40" length="20" type="BLANK" /><!-- B40 :: Passport Number -->
        <field name="B41" length="10" type="BLANK" /><!-- B41 :: Passport Expiration Date -->
        <field name="B42" length="25" type="BLANK" /><!-- B42 :: Foreign Tax Payer ID -->
        <field name="B43" length="2" type="STATIC" value="US" /><!-- B43 :: Tax Residence Country Code -->
        <field name="B44" length="25" type="BLANK" /><!-- B44 :: Tax Residence Country Name (NOTE: Should be non-blank if B43 is blank or invalid) -->
        <field name="B45" length="1" type="BLANK" /><!-- B45 :: Claim Treaty benefits -->
        <field name="B46" length="2" type="BLANK" /><!-- B46 :: State of employment -->
        <field name="B47" length="1" type="BLANK" /><!-- B47 :: Personal exemption -->
        <field name="B48" length="1" type="BLANK" /><!-- B48 :: Pay Cycle -->
        <field name="B49" length="10" type="BLANK" /><!-- B49 :: Estimated Annual Income -->
        <field name="B50" length="1" type="BLANK" /><!-- B50 :: Self Employment Fixed base office -->
        <field name="B51" length="3" type="BLANK" /><!-- B51 :: No. of day office available -->
        <field name="B52" length="1" type="BLANK" /><!-- B52 :: Recipient of grant -->
        <field name="B53" length="1" type="BLANK" /><!-- B53 :: Full time program -->
        <field name="B54" length="1" type="BLANK" /><!-- B54 :: LPR steps -->
        <field name="B55" length="1" type="BLANK" /><!-- B55 :: Tax Home Connection -->
        <field name="B56" length="1" type="BLANK" /><!-- B56 :: Trainee Type -->
        <field name="B57" length="2" type="BLANK" /><!-- B57 :: Student Type -->
        <field name="B58" length="10" type="BLANK" /><!-- B58 :: User 1 (Text) -->
        <field name="B59" length="10" type="BLANK" /><!-- B59 :: User 2 (Text) -->
        <field name="B60" length="10" type="BLANK" /><!-- B60 :: User 3 (Currency) -->
        <field name="B61" length="1" type="BLANK" /><!-- B61 :: User 4 (Check box) -->
        <field name="B62" length="1" type="BLANK" /><!-- B62 :: User 5 (Check box) -->
        <field name="B63" length="10" type="BLANK" /><!-- B63 :: User 6 (Date) -->
        <field name="B64" length="10" type="BLANK" /><!-- B64 :: User 7 (Date) -->
        <field name="B65" length="10" type="BLANK" /><!-- B65 :: User 8 (Currency) -->
        <field name="B66" length="4" type="BLANK" /><!-- B66 :: NQI/FLW-THR ID -->
        <field name="B67" length="40" type="BLANK" /><!-- B67 :: NQI/FLW-THR Name Line 1 -->
        <field name="B68" length="4" type="BLANK" /><!-- B68 :: Payer ID -->
        <field name="B69" length="40" type="BLANK" /><!-- B69 :: Payer Name -->
        <field name="B70" length="10" type="STATIC" value="KFS" /><!-- B70 :: Record Source -->
        <field name="B71" length="20" type="BLANK" /><!-- B71 :: Campus -->
        <field name="B72" length="9" type="DERIVED" value="ein" /><!-- B72 :: EIN -->
        <field name="B73" length="4" type="BLANK" /><!-- B73 :: Primary Withholding Agent's ID -->
        <field name="B74" length="40" type="BLANK" /><!-- B74 :: Primary Withholding Agent's Name -->
        <!--
            ==== NOTE: The current spec does not have any fields with names B75 through B96. ====
         -->
        <field name="B97" length="25" type="BLANK" /><!-- B97 :: Filler -->
        <field name="B98" length="1" type="STATIC" value="I" /><!-- B98 :: I (just the letter "I") -->
        <field name="B99" length="2" type="BLANK" /><!-- B99 :: Carriage Return/Line Feed (can be spaces instead if writing to a flat ASCII file) -->
    </section>
    <!--
        ================================================================================================================
        This section represents a 1042S detail record (header record fields A1 through A13, plus fields D1 through D99).
        ================================================================================================================
     -->
    <section length="598" hasExactLength="true">
        <field name="D1" length="4" type="DETAIL" value="reportYear" /><!-- D1 :: Tax Year -->
        <field name="D2" length="2" type="DERIVED" value="incomeCode" /><!-- D2 :: Income Code -->
        <field name="D3" length="1" type="DETAIL" value="incomeCodeSubType" /><!-- D3 :: Income Code subtype -->
        <field name="D4" length="1" type="STATIC" value="0" /><!-- D4 :: Type of Applicable Tax (0 - Reporting under Chapter 3 (NRA), 1 - Reporting Under Chapter 4 (FATCA)) -->
        <field name="D5" length="2" type="DERIVED" value="chapter3ExemptionCode" /><!-- D5 :: Chapter 3 Exemption Code -->
        <field name="D6" length="5" type="DERIVED" value="chapter3TaxRate" /><!-- D6 :: Chapter 3 Tax Rate -->
        <field name="D7" length="2" type="DERIVED" value="chapter4ExemptionCode" /><!-- D7 :: Chapter 4 Exemption Code -->
        <field name="D8" length="5" type="STATIC" value="00.00" /><!-- D8 :: Chapter 4 Tax Rate -->
        <field name="D9" length="10" type="DERIVED" value="grossAmount" /><!-- D9 :: Gross amount paid to individual -->
        <field name="D10" length="10" type="STATIC" value="0" /><!-- D10 :: Withholding Allowance (only valid for income types 15 or 16, code 0 for other types) -->
        <field name="D11" length="10" type="DERIVED" value="fedTaxWithheldAmount" /><!-- D11 :: Federal Withheld Amount -->
        <field name="D12" length="10" type="BLANK" /><!-- D12 :: Withholding by other agent (for future use) -->
        <field name="D13" length="10" type="DERIVED" value="stateIncomeTaxWithheldAmount" /><!-- D13 :: State Withheld Amount -->
        <field name="D14" length="10" type="BLANK" /><!-- D14 :: Refund Amount -->
        <field name="D15" length="10" type="BLANK" /><!-- D15 :: Out of system treaty benefit -->
        <field name="D16" length="10" type="BLANK" /><!-- D16 :: Tax Assumed by Withholding Agent -->
        <field name="D17" length="2" type="DERIVED" value="stateCode" /><!-- D17 :: State Code -->
        <field name="D18" length="2" type="BLANK" /><!-- D18 :: Country Code  -->
        <field name="D19" length="25" type="DETAIL" value="paymentCountryName" /><!-- D19 :: Country Name (NOTE: Should be non-blank if D18 Country Code is not given) -->
        <field name="D20" length="10" type="DETAIL" value="paymentDate" /><!-- D20 :: Cycle Date (Payment Date) -->
        <field name="D21" length="1" type="STATIC" value="A" /><!-- D21 :: Record Status (F=Forecast, A=Actual, O=Out of System, R=Refund) -->
        <field name="D22" length="1" type="STATIC" value="Y" /><!-- D22 :: Payment Type (Y=Year-to-date, C=Cycle only) -->
        <field name="D23" length="4" type="BLANK" /><!-- D23 :: NQI/FLW-THR ID -->
        <field name="D24" length="40" type="BLANK" /><!-- D24 :: NQI/FLW-THR Name Line 1 -->
        <field name="D25" length="4" type="BLANK" /><!-- D25 :: Payer ID -->
        <field name="D26" length="40" type="BLANK" /><!-- D26 :: Payer Name -->
        <field name="D27" length="4" type="BLANK" /><!-- D27 :: Primary Withholding Agent's ID -->
        <field name="D28" length="40" type="BLANK" /><!-- D28 :: Primary Withholding Agent's Name -->
        <field name="D29" length="10" type="STATIC" value="KFS" /><!-- D29 :: Record Source -->
        <field name="D30" length="20" type="BLANK" /><!-- D30 :: Campus -->
        <field name="D31" length="9" type="DERIVED" value="ein" /><!-- D31 :: EIN -->
        <!--
            ==== NOTE: The current spec does not have any fields with names D32 through D96. ====
         -->
        <field name="D97" length="25" type="BLANK" /><!-- D97 :: Filler -->
        <field name="D98" length="1" type="STATIC" value="P" /><!-- D98 :: P (just the letter "P") -->
        <field name="D99" length="2" type="BLANK" /><!-- D99 :: Carriage Return/Line Feed (can be spaces instead if writing to a flat ASCII file) -->
    </section>
</taxOutputDefinition>