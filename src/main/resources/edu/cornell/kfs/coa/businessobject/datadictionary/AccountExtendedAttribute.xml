    <bean id="AccountExtendedAttribute-inquiryDefinition" parent="AccountExtendedAttribute-inquiryDefinition-parentBean"/>

  <bean id="AccountExtendedAttribute-inquiryDefinition-parentBean" abstract="true" parent="InquiryDefinition">
    <property name="title" value="Sub-Fund Group Program Inquiry"/>
    <property name="inquirySections">
      <list>
        <bean parent="InquirySectionDefinition">
          <property name="title" value=""/>
          <property name="numberOfColumns" value="1"/>
          <property name="inquiryFields">
            <list>
              <bean parent="FieldDefinition" p:attributeName="programCode"/>
              <bean parent="FieldDefinition" p:attributeName="subFundGroupCode"/>
            </list>
          </property>
        </bean>
      </list>
    </property>
  </bean>

--><!-- Business Object Lookup Definition --><!--


  <bean id="AccountExtendedAttribute-lookupDefinition" parent="AccountExtendedAttribute-lookupDefinition-parentBean"/>

  <bean id="AccountExtendedAttribute-lookupDefinition-parentBean" abstract="true" parent="LookupDefinition">
    <property name="title" value="Sub-Fund Group Program Lookup"/>
    
    <property name="defaultSort">
      <bean parent="SortDefinition">
        <property name="attributeNames">
          <list>
            <value>programCode</value>
          </list>
        </property>
        <property name="sortAscending" value="false"/>
      </bean>
    </property>
    <property name="lookupFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="programCode"/>
        <bean parent="FieldDefinition" p:attributeName="subFundGroupCode"/>
      </list>
    </property>
    <property name="resultFields">
      <list>
        <bean parent="FieldDefinition" p:attributeName="programCode"/>
     	<bean parent="FieldDefinition" p:attributeName="subFundGroupCode"/>
      </list>
    </property>
  </bean> 
     
