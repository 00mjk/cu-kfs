<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

  <bean id="VendorSupplierDiversity" parent="VendorSupplierDiversity-parentBean">
  <!--   <property name="businessObjectClass" value="edu.cornell.kfs.vnd.businessobject.CuVendorSupplierDiversity"/> -->
	<property name="attributes">
      <list merge = "true">
        <ref bean="VendorSupplierDiversity-vendorSupplierDiversityExpirationDate"/>
      </list>
    </property>
  </bean>

  <!-- Attribute Definitions -->
  <bean id="VendorSupplierDiversity-vendorSupplierDiversityExpirationDate" parent="VendorSupplierDiversity-vendorSupplierDiversityExpirationDate-parentBean"/>

  <bean id="VendorSupplierDiversity-vendorSupplierDiversityExpirationDate-parentBean" abstract="true" parent="GenericAttributes-genericDate">
    <property name="name" value="vendorSupplierDiversityExpirationDate"/>
    <property name="label" value="Supplier Diversity Certification Expiration Date"/>
    <property name="shortLabel" value="Supplier Diversity Certification Expiration Date"/>
    <property name="required" value="true"/>
  </bean> 
  
  
</beans>
